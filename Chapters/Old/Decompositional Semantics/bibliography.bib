@article{ward_response_1983,
	title = {Response tempo and separable-integral responding: Evidence for an integral-to-separable processing sequence in visual perception.},
	volume = {9},
	doi = {10.1037/0096-1523.9.1.103},
	abstract = {A restricted classification paradigm, in which similarity-based (integral) and dimension-based (separable) classifications were pitted against one another, was used with the dimensions of length and density {(Exps} {Iâ€“III)} and size and brightness {(Exp} {III).} Ss were 46 undergraduates and 90 kindergarten, 2nd-, and 5th-grade children. Rapid responding was associated with fewer dimensional and more similarity responses than was slow responding. This held when response tempo was simply measured and when it was manipulated. Results are consistent with models of dimensional processing in which holistic, integral processing precedes analytic dimensional processing.},
	number = {1},
	journal = {Journal of Experimental Psychology: Human Perception and Performance},
	author = {Ward, Thomas B.},
	year = {1983},
