@article{smith_incomplete_2012,
	title = {Incomplete Understanding of Concepts: The Case of the Derivative},
	abstract = {Many philosophers have discussed the ability of thinkers to think
thoughts that the thinker cannot justify because the thoughts involve
concepts that the thinker incompletely understands. A standard ex-
ample of this phenomenon, the one around which this paper revolves,
involves the concept of the derivative in the early days of the calculus:
Newton and Leibniz incompletely understood the derivative concept
and, hence, as Berkeley noted, they could not justify their thoughts in-
volving it. Later, so the story goes, Weierstrass justied their thoughts
by giving a correct explication of the derivative concept. This paper
discusses various accounts of how one should think about how a thinker
manages to think with a concept that they incompletely understands
{\textbar} including Christopher Peacocke's {\textbackslash}implicit conceptions" approach
and Georges Rey's {\textbackslash}deference to optimal theory" approach {\textbar} and
nds them wanting in the case at hand. Part of the overlooked com-
plexity is that there are many derivative concepts and it is unclear in
virtue of what a thinker would be thinking with one of them rather
than another. After critical evaluation of standard accounts, this paper
suggests a novel account of how one should think about the derivative
concepts with which Newton and Leibniz thought and how Weierstrass
could have managed to justify their thoughts even if their thoughts did
not involve the same derivative concept as Weierstrass'.},
	journal = {unpublished},
	author = {Smith, Sheldon},
	month = aug,
	year = {2012}
},

@article{nosofsky_exemplar-based_1997,
	title = {An Exemplar-Based Random Walk Model of Speeded Classification},
	volume = {104},
	abstract = {The authors propose and test an exemplar-based random walk model for predicting response times
in tasks of speeded, multidimensional perceptual classification. The model combines elements of
{R.M.} Nosofsky's (1986) generalized context model of categorization and G. D. Logan's (1988)
instance-based model of automaticity. In the model, exemplars race among one another to be retrieved
from memory, with rates determined by their similarity to test items. The retrieved exemplars provide
incremental information that enters into a random walk process for making classification decisions.
The model predicts correctly effects of within- and between-categories similarity, individual-object familiarity, and extended practice on classification response times. It also builds bridges between the domains of categorization and automaticity.},
	number = {2},
	journal = {Psychological Review},
	author = {Nosofsky, Robert M. and Palmeri, Thomas J.},
	year = {1997},
	keywords = {categories, categorization, concept-formation, exemplar theory, hybrid theories, Need to read, Psychology, rule-based theories},
